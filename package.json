{
  "name": "ng-lightning-app",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "pug": "gulp pug",
    "pug:watch": "gulp pug:watch",
    "lib:watch": "ng build ng-lightning --watch",
    "app:serve": "wait-on ./dist/ng-lightning/package.json && ng serve --port 1111",
    "prestart": "rimraf dist && npm run pug",
    "start": "run-p -r lib:watch pug:watch app:serve",
    "prebuild": "npm run pug",
    "build": "ng build",
    "pretest": "npm run pug",
    "test": "ng test ng-lightning --watch=false",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "docs": "ts-node --project scripts/tsconfig.json scripts/generate-docs.ts"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.1",
    "@angular/common": "^7.0.1",
    "@angular/compiler": "^7.0.1",
    "@angular/core": "^7.0.1",
    "@angular/forms": "^7.0.1",
    "@angular/http": "^7.0.1",
    "@angular/platform-browser": "^7.0.1",
    "@angular/platform-browser-dynamic": "^7.0.1",
    "@angular/router": "^7.0.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "rxjs": "~6.3.3",
    "tether": "^1.4.5",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular-devkit/build-ng-packagr": "~0.8.0",
    "@angular/cli": "~7.0.3",
    "@angular/compiler-cli": "^7.0.1",
    "@angular/language-service": "^7.0.1",
    "@salesforce-ux/design-system": "^2.7.4",
    "@types/jasmine": "^2.8.9",
    "@types/jasminewd2": "^2.0.5",
    "@types/node": "~8.9.4",
    "@types/tether": "^1.4.4",
    "codelyzer": "^4.5.0",
    "del": "^3.0.0",
    "fs-extra": "^7.0.0",
    "glob": "^7.1.3",
    "gulp": "^4.0.0",
    "gulp-changed": "^3.2.0",
    "gulp-data": "^1.3.1",
    "gulp-pug": "^4.0.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "markdown-it": "^8.4.2",
    "ng-packagr": "^4.1.0",
    "npm-run-all": "^4.1.3",
    "prismjs": "^1.15.0",
    "protractor": "~5.4.0",
    "pug": "^2.0.3",
    "rimraf": "^2.6.2",
    "ts-node": "~7.0.0",
    "tsickle": "^0.33.0",
    "tslib": "^1.9.0",
    "tslint": "~5.11.0",
    "typescript": "~3.1.3",
    "wait-on": "^3.1.0"
  }
}
